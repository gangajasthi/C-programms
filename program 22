#include <iostream>
using namespace std;

// Function template
template <typename T>
T add(T a, T b) {
    return a + b;
}

int main() {
    // Using function template with int
    int x = 5, y = 10;
    cout << "Sum of integers: " << add(x, y) << endl;

    // Using function template with double
    double p = 3.5, q = 4.7;
    cout << "Sum of doubles: " << add(p, q) << endl;

    // Using function template with float
    float f1 = 2.3f, f2 = 5.1f;
    cout << "Sum of floats: " << add(f1, f2) << endl;

    return 0;
}
