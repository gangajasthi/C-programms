#include <iostream>
#include <deque>
using namespace std;

int main() {
    deque<int> dq;
    dq.push_back(10);     
    dq.push_back(20);
    dq.push_front(5);      
    dq.push_front(2);

    cout << "Deque after insertion: ";
    for (int val : dq) {
        cout << val << " ";
    }
    cout << endl;
    dq.pop_back();         
    dq.pop_front();        

    cout << "Deque after deleting front and back elements: ";
    for (int val : dq) {
        cout << val << " ";
    }
    cout << endl;

    cout << "Front element: " << dq.front() << endl;
    cout << "Back element: " << dq.back() << endl;

    cout << "Traversing deque using iterator: ";
    for (deque<int>::iterator it = dq.begin(); it != dq.end(); ++it) {
        cout << *it << " ";
    }
    cout << endl;

    cout << "Size of deque: " << dq.size() << endl;
    cout << "Is deque empty? " << (dq.empty() ? "Yes" : "No") << endl;

    return 0;
}
