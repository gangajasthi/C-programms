#include<iostream>
using namespace std;
int main(){
	int numerator,denominator;
	double res;
	cout<<"Enter Numerator:";
	cin>>numerator;
	cout<<"Enter Denominator:";
	cin>>denominator;
	try{
		if(denominator==0)
	       throw denominator;
	    res=(double)numerator/denominator;
	    cout<<"Result= "<<res<<endl;
	}
	catch (int e){
		cout<<"Error: Division by zero is not allowed"<<endl;
	}
	cout<<"End program...."<<endl;
	return 0;
}
